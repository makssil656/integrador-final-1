          <td>
            <div class="d-flex">
              <div class="d-inline icon">
                <a href="/historias/Listar/{{_id}}" class="btn btn-primary"style="margin-right: 5px;">
                 <img src="http://localhost:3000/svg/editar_paciente.svg" alt="Editar paciente" class="iccon" />
                </a>
              </div>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>