<div class="mt-3">
  <h1>historias</h1>
</div>
<hr />
<div
  class="table-responsive container justify-content-center align-items-center col-lg-10"
>
  <table id="tabla1" class="">
    <thead>
      <tr>
        <th>DNI</th>
        <th>Apellidos</th>
        <th>Nombres</th>
        <th>Edad</th>
        <th>Sexo</th>
        <th>Orientaci√≥n</th>
        <th>Fecha Nac.</th>
        <th>Acciones</th>
      </tr>
    </thead>
    <tbody>
      {{#each pacientes}}
        <tr>
          <td>{{DNI}}</td>
          <td>{{apellidos}}</td>
          <td>{{nombres}}</td>
          <td>{{Edad}}</td>
          <td>{{Sexo}}</td>
          <td>{{orientacion_sex}}</td>
